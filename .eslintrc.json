{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended", 
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "semi": ["error", "always"],
        "indent": ["error", 2],
        "no-multiple-empty-lines": ["error", { "max": 1 }],
        "quotes": ["error", "single"]
    }
}
